ment.querySelector('.welcome-text h1')\n    let textP = document.querySelector('.welcome-text p')\n    \n    window.addEventListener('scroll', e => {\n      if(window.scrollY + 150 > welcome.offsetTop && window.scrollY < welcome.offsetTop + welcome.offsetHeight) {\n        let floating = (window.scrollY - welcome.offsetTop) / welcome.offsetHeight\n        floating = floating.toFixed(1) * 10\n\n        textHeading.style.transform = `translateX(${floating}px)`\n        textP.style.transform = `translateX(-${floating}px)`\n      }\n    })\n\n  }\n}\n</script>\n\n<style>\n  .welcome-container {\n    display: flex;\n    justify-content: flex-end;\n    padding: 1rem;\n  }\n  .potato-bg {\n    width: 85vw;\n    height: 100vh;\n  }\n  #potato-img {\n    background: url('../assets/img/Holiday-Hasselback-Potatoes.jpg') no-repeat center;\n    width: 100%;\n    height: 100%;\n    border-radius: 10px;\n    background-size: cover;\n  }\n  .welcome-text {\n    position: absolute;\n    top: 40vh;\n    left: 5vw;\n  }\n  .welcome-text h1 {\n    font-size: 4rem;\n    transition: linear 1000ms;\n  }\n  .welcome-text p {\n    font-size: 1.5rem;\n    transition: linear 1000ms;\n  }\n</style>"]}]}