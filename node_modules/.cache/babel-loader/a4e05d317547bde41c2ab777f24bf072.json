\n</template>\n\n<script>\nexport default {\n  name: 'Intro',\n  mounted() {\n    let intro = document.getElementById('intro-container')\n    let bg1 = document.getElementById('bg1')\n    let bg2 = document.getElementById('bg2')\n\n    window.addEventListener('scroll', e => {\n      if(window.scrollY + 150 > intro.offsetTop && window.scrollY < intro.offsetTop + intro.offsetHeight) {\n        let floating = (window.scrollY - intro.offsetTop) / intro.offsetHeight\n        floating = floating.toFixed(2) * 25\n\n        bg1.style.transform = `translateX(-${floating}px)`\n        bg2.style.transform = `translateX(${floating}px)`\n      }\n    })\n  },\n  methods: {\n    \n  }\n}\n</script>\n\n<style>\n  .intro-wrap {\n    width: 100%;\n  }\n  .intro-container {\n    display: flex;\n    justify-content: space-between;\n    width: 100%;\n    height: 100vh;\n    padding: 1rem;\n  }\n  .intro-text {\n    position: relative;\n    top: 40vh;\n    left: 1vw;\n    margin: 1rem;\n    padding: 1rem;\n  }\n  .intro-text h1 {\n    font-size: 2rem;\n  }\n  .intro-text p {\n    font-size: 1.25rem;\n  }\n  .intro-gallery {\n    display: flex;\n    margin: 1rem;\n  }\n  .intro-img {\n    border-radius: 10px;\n    overflow: hidden;\n    position: relative;\n    box-shadow: 1px 1px 3px rgba(0, 0, 0, 0.3);\n  }\n  .bg1 {\n    background: url('../assets/img/Baked-Potato-Wedges.jpg') no-repeat center;\n    background-size: cover;\n    width: 350px;\n    height: 400px;\n    top: 25vh;\n    z-index: 1;\n    transition: linear 1000ms;\n  }\n  .bg2 {\n    background: url('../assets/img/hasselback-potatoes.jpg') no-repeat center;\n    background-size: cover;\n    width: 400px;\n    height: 400px;\n    top: 12vh;\n    right: 10vw;\n    transition: linear 1000ms;\n  }\n  \n</style>"],"sourceRoot":""}]}